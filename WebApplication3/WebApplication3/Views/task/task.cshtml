
@{
    ViewBag.Title = "task";

}


<table class="table  table-hover">
    <tr>
        <th>Id</th>
        <th>Name</th>
        <th>Major</th>
        <th>Faculity</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Id</td>
            <td>@item.Name</td>
            <td>@item.Major</td>
            <td>@item.Faculity</td>
        </tr>
    }

    </table>































 

